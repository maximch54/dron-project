<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.collections.*?>
<?import javafx.scene.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.control.TextArea?>

<AnchorPane layoutX="1.0" layoutY="1.0"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<VBox prefHeight="137.0" prefWidth="451.0">
			<children>
				<Label prefHeight="31.0" prefWidth="453.0"
					text="Please choose or enter path for POSTman file with values." />
				<HBox prefHeight="28.0" prefWidth="451.0">
					<children>
						<TextField fx:id="tfPostmanValues" prefHeight="26.0"
							prefWidth="350.0" />
						<Button mnemonicParsing="false" onAction="#chooseFileWithValues"
							text="Choose" />
					</children>
				</HBox>
				<Label text="Please choose or enter path for POSTman file with requests." />
				<HBox prefHeight="28.0" prefWidth="451.0">
					<children>
						<TextField fx:id="tfPostmanRequests" prefHeight="26.0"
							prefWidth="350.0" />
						<Button mnemonicParsing="false" onAction="#chooseFileWithRequests"
							text="Choose" />
					</children>
				</HBox>
				<HBox alignment="TOP_CENTER" prefHeight="28.0" prefWidth="451.0">
					<children>
						<Button onAction="#importPostmanData" mnemonicParsing="false"
							text="Import" />
						<Button fx:id="btnCancel" onAction="#cancelImportPostmanDialog"
							mnemonicParsing="false" text="Cancel" />
					</children>
				</HBox>
			</children>
		</VBox>
	</children>
</AnchorPane>
